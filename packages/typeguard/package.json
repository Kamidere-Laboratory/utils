{
  "name": "@kamidere/typeguard",
  "version": "0.0.2",
  "description": "typeguard",
  "main": "dist/esm/main.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.js",
      "types": "./dist/types/main.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "LICENSE"

  ],
  "types": "./dist/types/main.d.ts",
  "repository": "https://github.com/Kamidere-Laboratory/utils/",
  "author": "Kamide.re Laboratory",
  "contributors": [
    {"name": "Robert \"RunAge\" Gajda", "url": "https://github.com/RunAge"},
    {"name": "Bartosz \"hendas\" Marciniec", "url": "https://github.com/hendas"}
  ],
  "license": "MIT",
  "private": false,
  "keywords": [],
  "scripts": {
    "start:dev": "NODE_ENV=development node --loader ts-node/esm --no-warnings src/main.ts",
    "build": "rimraf ./dist && tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json && cp ./templates/cjs.json ./dist/cjs/package.json",
    "eslint": "eslint --ext .ts"
  },
  "dependencies": {
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
