{
  "$schema": "http://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "all",
  "printWidth": 120,
  "bracketSameLine": true,
  "endOfLine": "lf",
  "parser": "typescript",
  "arrowParens": "always",
  "bracketSpacing": true
}
